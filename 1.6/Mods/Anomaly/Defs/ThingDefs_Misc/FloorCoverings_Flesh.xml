<?xml version="1.0" encoding="utf-8" ?>
<Defs>



  <ThingDef ParentName="BuildingFloorCoveringMediumBase">
    <defName>AM_FleshSlab_Medium</defName>
    <label>flesh slab (medium)</label>
    <description>A stone bas relief of flesh imagery for decorating a floor.</description>
    <dominantStyleCategory>AM_Flesh</dominantStyleCategory>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/BuildingStyles/Flesh/Slab/Medium</texPath>
      <drawSize>(3,3)</drawSize>
    </graphicData>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <stuffCategories>
      <li>Stony</li>
	
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
  </ThingDef>

  <ThingDef ParentName="BuildingFloorCoveringBroadBase">
    <defName>AM_FleshSlab_Broad</defName>
    <label>flesh slab (broad)</label>
    <description>A stone bas relief of flesh imagery for decorating a floor.</description>
    <dominantStyleCategory>AM_Flesh</dominantStyleCategory>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/BuildingStyles/Flesh/Slab/Broad</texPath>
      <drawSize>(4,4)</drawSize>
    </graphicData>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <stuffCategories>
      <li>Stony</li>
	
    </stuffCategories>
    <costStuffCount>150</costStuffCount>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
  </ThingDef>
  
  
</Defs>