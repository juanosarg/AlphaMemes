<?xml version="1.0" encoding="utf-8" ?>
<Defs>
		<!--Precepts-->
	<PreceptDef>
		<defName>AM_InsectoidBurial</defName>
		<label>hive burial</label>
		<description>A gathering to witness our friend offered to the greater powers; Insects!</description>
		<issue>Ritual</issue>		
		<iconPath>UI/Rituals/AM_FuneralInsectoid</iconPath>
		<preceptClass>Precept_Ritual</preceptClass>
		<ritualPatternBase>AM_InsectoidFuneralPattern</ritualPatternBase>
		<allowDuplicates>false</allowDuplicates>
		<countsTowardsPreceptLimit>false</countsTowardsPreceptLimit>
		<ignoreNameUniqueness>true</ignoreNameUniqueness>
		<selectionWeight>1.0</selectionWeight>
		<displayOrderInImpact>100</displayOrderInImpact>
		<impact>Medium</impact>
		<requiredMemes>
			
		</requiredMemes>
		<ignoreLimitsInEditMode>false</ignoreLimitsInEditMode>
		<receivesExpectationsQualityOffset>true</receivesExpectationsQualityOffset>
		<skipOpportunityLettersBeforeDay>0</skipOpportunityLettersBeforeDay>
		<extraTextPawnDeathLetter>{PAWN_nameDef} should be offered to the insectoids so {PAWN_possessive} friends and family will know they are with the superior race</extraTextPawnDeathLetter>
		<modExtensions>
		<li Class="AlphaMemes.FuneralPreceptExtension">
			<isColonistFuneral>true</isColonistFuneral>
			<addNoCorpseFuneral>true</addNoCorpseFuneral>
			<replaceConflictRituals>true</replaceConflictRituals> 
			<!--Animals-->
			<allowAnimals>false</allowAnimals>			
			<!--Conflicts-->
		</li>
		</modExtensions>
	</PreceptDef>
	
	


</Defs>