<Defs>



  <ThingDef ParentName="BuildingFloorCoveringMediumBase">
    <defName>AM_JewishSlab_Medium</defName>
    <label>jewish slab (medium)</label>
    <description>A stone bas relief of jewish imagery for decorating a floor.</description>
    <dominantStyleCategory>AM_Jewish</dominantStyleCategory>
    <graphicData>
      <graphicClass>Graphic_Random</graphicClass>
      <texPath>Things/Building/BuildingStyles/Jewish/Slab/Medium</texPath>
      <drawSize>(3,3)</drawSize>
    </graphicData>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <stuffCategories>
      <li>Stony</li>
    </stuffCategories>
    <costStuffCount>100</costStuffCount>
    <researchPrerequisites>
      <li>Stonecutting</li>
    </researchPrerequisites>
  </ThingDef>

